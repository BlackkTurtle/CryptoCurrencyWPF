<Window x:Class="CryptoCurrencyWPF.Views.ConvertCurrencies"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CryptoCurrencyWPF.Views"
        mc:Ignorable="d"
        Title="ConvertCurrencies" Height="187" Width="430">
    <Grid>
        <ComboBox x:Name="ComboBoxCurrencies1" HorizontalAlignment="Left" Height="27" Margin="75,20,0,0" VerticalAlignment="Top" Width="145" IsEditable="True" FontSize="16" Text="ddvsfsd"/>
        <ComboBox x:Name="ComboBoxCurrencies2" HorizontalAlignment="Left" Height="27" Margin="275,20,0,0" VerticalAlignment="Top" Width="145" FontSize="16" Text="ddvsfsd" IsEditable="True"/>
        <Label Content="From:" HorizontalAlignment="Left" Margin="23,18,0,0" VerticalAlignment="Top" FontSize="16"/>
        <Label Content="To:" HorizontalAlignment="Left" Height="29" Margin="235,18,0,0" VerticalAlignment="Top" Width="35" FontSize="16"/>
