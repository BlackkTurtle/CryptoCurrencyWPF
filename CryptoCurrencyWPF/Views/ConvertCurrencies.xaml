        <Label Content="Quantity:" HorizontalAlignment="Left" Height="36" Margin="10,70,0,0" VerticalAlignment="Top" Width="78" FontSize="16"/>
        <TextBox x:Name="TextBoxQuantity" HorizontalAlignment="Left" Height="28" Margin="88,74,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="132" FontSize="16" Text="dasdasdsa"/>
        <Label Content="=" HorizontalAlignment="Left" Margin="235,72,0,0" VerticalAlignment="Top" FontSize="16"/>
        <TextBlock x:Name="TextBlockResult" HorizontalAlignment="Left" Height="28" Margin="278,74,0,0" TextWrapping="Wrap" Text="daddgs" VerticalAlignment="Top" Width="110" IsEnabled="False" FontSize="16"/>

    </Grid>
</Window>
